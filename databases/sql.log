CREATE TABLE scheduler_worker(
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    worker_name CHAR(512) UNIQUE,
    first_heartbeat TIMESTAMP,
    last_heartbeat TIMESTAMP,
    status CHAR(512),
    is_ticker CHAR(1),
    group_names TEXT
);
