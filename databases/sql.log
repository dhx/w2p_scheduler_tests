CREATE TABLE scheduler_task(
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    application_name CHAR(512),
    task_name CHAR(512),
    group_name CHAR(512),
    status CHAR(512),
    function_name CHAR(512),
    uuid CHAR(512) UNIQUE,
    args TEXT,
    vars TEXT,
    enabled CHAR(1),
    start_time TIMESTAMP,
    next_run_time TIMESTAMP,
    stop_time TIMESTAMP,
    repeats INTEGER,
    retry_failed INTEGER,
    period INTEGER,
    timeout INTEGER,
    sync_output INTEGER,
    times_run INTEGER,
    times_failed INTEGER,
    last_run_time TIMESTAMP,
    assigned_worker_name CHAR(512)
);
success!
timestamp: 2012-08-17T21:56:25.926845
CREATE TABLE scheduler_run(
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    scheduler_task INTEGER REFERENCES scheduler_task (id) ON DELETE CASCADE,
    status CHAR(512),
    start_time TIMESTAMP,
    stop_time TIMESTAMP,
    output TEXT,
    result TEXT,
    traceback TEXT,
    worker_name CHAR(512)
);
success!
